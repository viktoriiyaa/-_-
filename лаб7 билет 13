#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int A[N];
    for (int i = 0; i < N; i++) {
        scanf("%d", &A[i]);
    }

    for (int i = 1; i < N; i += 2) {
        printf("%d ", A[i]);
    }

    for (int i = 0; i < N; i += 2) {
        printf("%d ", A[i]);
    }

    printf("\n");
    return 0;
}
