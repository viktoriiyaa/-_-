#include <iostream>
#include <vector>

int main() {
    int M;
    std::cout << "Введите размер массива: ";
    std::cin >> M;

    std::vector<int> A(M);
    std::cout << "Введите элементы массива: ";
    for (int i = 0; i < M; ++i) {
        std::cin >> A[i];
    }


    for (int i = 0; i < M; i += 2) {
        std::cout << A[i] << " ";
    }


    for (int i = 1; i < M; i += 2) {
        std::cout << A[i] << " ";
    }

    std::cout << std::endl;
    return 0;
}
